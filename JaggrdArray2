using System;

namespace Project
{
    internal class JaggedArray
    {
        public static void jaggedDeclaration()
        {
            int[][] num = new int[][]
            {
                new int []{1, 2, 3},
                new int []{4, 5, 77, 88},
                new int []{6, 7}
            };

            Console.WriteLine("Jagged Array Elements:\n");

            // Correct way to print elements
            foreach (var row in num)
            {
                foreach (var value in row)
                {
                    Console.Write(value + " ");
                }
                Console.WriteLine(); // Move to next line for next row
            }
        }

        public static void Main(string[] args)
        {
            jaggedDeclaration();
        }
    }
}
